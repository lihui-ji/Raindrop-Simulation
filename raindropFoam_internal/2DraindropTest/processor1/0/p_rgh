FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    right
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 0;
        value           uniform 0;
    }
    bot
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 0;
        value           uniform 0;
    }
    top
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0;
    }
}


