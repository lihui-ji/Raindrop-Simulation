FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application     raindropFoam;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         2e-1;

//endTime       0.0001;

deltaT          1e-4;

writeControl    adjustableRunTime;

writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           1;
maxAlphaCo      1;

maxDeltaT       1;

threshold       0.9;

// ************************************************************************* //

initialRaindropCenterx	-0.000000;

initialRaindropCentery	0.000000;

initialRaindropCenterz	-0.020000;
